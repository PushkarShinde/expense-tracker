# Create .gitignore file
cat > .gitignore << 'EOF'
# Build directories
build/
bin/
dist/

# Object files
*.o
*.ko
*.obj
*.exe
*.dll
*.so
*.so.*
*.dylib

# Precompiled Headers
*.gch
*.pch

# Libraries
*.lib
*.a
*.la
*.lo

# Debug files
*.dSYM/
*.su
*.idb
*.pdb

# Kernel Module Compile Results
*.mod*
*.cmd
.tmp_versions/
modules.order
Module.symvers
Mkfile.old
dkms.conf

# Backup files
*~
*.bak
*.swp
*.swo

# IDE files
.vscode/
.idea/
*.swp
*.swo

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Logs
*.log
EOF